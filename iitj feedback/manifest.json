{
  "manifest_version": 3,
  "name": "IITJ ERP Feedback Automation",
  "version": "1.1",
  "description": "Automatically fills out feedback on IIT Jodhpur ERP.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Auto-Fill Feedback",
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
